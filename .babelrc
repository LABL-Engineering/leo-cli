{
  "presets": [
    [
      "@babel/preset-react",
      {
        "throwIfNamespace": false, // defaults to true
        "runtime": "automatic" // defaults to classic
        "importSource": "custom-jsx-library" // defaults to react (only in automatic runtime)
      }
    ],    
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "20"
        },
  			"modules": "auto",
  			"sourceType": "unambiguous",
  			"useBuiltIns": "usage",
  			"corejs": "3.38.1"
  		}
    ]
  ],
	"plugins": ["@babel/plugin-proposal-export-namespace-from", "@babel/plugin-transform-modules-commonjs", "@babel/plugin-proposal-function-bind", "@babel/plugin-proposal-decorators"]  
}
